{
    "version": 2,
    "builds": [
      {
        "src": "server/dist/server.bundle.js",
        "use": "@vercel/node"
      },
      {
        "src": "client//dist/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server/dist/server.bundle.js"
      },
      {
        "src": "/(.*)",
        "dest": "client/dist/$1"
      }
    ]
  }
  