{
    "version": 2,
    "builds": [
      {
        "src": "/server/dist/server.bundle.js",
        "use": "@vercel/node"
      }
      
    ],  "routes": [
        {
          "src": "/v1/(.*)",
          "dest": "/server/dist/server.bundle.js"
        },
        {
          "src": "/(.*)",
          "dest": "client/dist/index.html"
        }
      ]
  }