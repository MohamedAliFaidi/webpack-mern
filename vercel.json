{
    "version": 2,
    "builds": [
      {
        "src": "/server/dist/server.bundle.js",
        "use": "@vercel/node"
      }
      
    ],  "routes": [
        {
          "src": "/(.*)",
          "dest": "/server/dist/server.bundle.js"
        },
        {
          "src": "/(.*)",
          "dest": "/client/dist"
        }
      ]
  }